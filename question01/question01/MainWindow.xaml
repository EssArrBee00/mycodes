<Window x:Class="question01.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:question01"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="400">
    <Grid Background="#FF105961">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Background="AliceBlue" Width="300">
            <Button Content="Create User" Click="Button_Click" Margin="0,0,0,15"></Button>
            <Button Content="Show All Users" Click="Button_Click_1" Margin="0,0,0,15"></Button>
            <Button Content="EXIT"></Button>
        </StackPanel>
    </Grid>
</Window>
